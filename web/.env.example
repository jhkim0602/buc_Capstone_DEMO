# Supabase 설정
# Supabase 프로젝트 URL (대시보드 > Project Settings > API에서 확인 가능)
NEXT_PUBLIC_SUPABASE_URL=https://oqepcpnkhhotkberqyhz.supabase.co

# Supabase Anon Key (클라이언트용 공개 키)
# 로그인, 데이터 읽기 등 클라이언트 사이드 작업에 사용됩니다.
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key

# Supabase Service Role Key (서버 사이드 전용)
# Next.js API Routes 등 서버 환경에서 관리자 권한으로 DB에 접근할 때 사용됩니다.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# LiveKit 설정 (화상 면접 기능용)
# 로컬 개발 시: ws://localhost:7880
NEXT_PUBLIC_LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# 실행 환경 설정
# development: 개발 환경, production: 배포 환경
NODE_ENV=development

# Fireworks AI 설정 (옵션)
# AI 모델을 통한 태그 생성 및 분석 기능에 사용됩니다.
FIREWORKS_API_KEY=your_fireworks_api_key

# AI Server WebSocket URL
# 실시간 인터뷰 등 AI 서버와 소켓 통신을 위한 주소입니다.
NEXT_PUBLIC_AI_WS_URL=ws://localhost:12393/client-ws

# Fireworks AI 모델 설정 (옵션)
FIREWORKS_MODEL=
